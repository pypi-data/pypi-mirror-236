{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Log Dependency Schema",
  "required": [
    "name",
    "version"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name Schema",
      "default": "",
      "examples": [
        "foobar"
      ],
      "pattern": "^(.*)$"
    },
    "version": {
      "$id": "#/properties/version",
      "type": "string",
      "title": "The version Schema",
      "default": "",
      "examples": [
        "foobar"
      ],
      "pattern": "^(.*)$"
    },
    "local_timestamp": {
      "$id": "#/properties/local_timestamp",
      "type": "integer",
      "title": "The Local_timestamp Schema",
      "default": 0,
      "examples": [
        1538390442607
      ]
    }
  }
}