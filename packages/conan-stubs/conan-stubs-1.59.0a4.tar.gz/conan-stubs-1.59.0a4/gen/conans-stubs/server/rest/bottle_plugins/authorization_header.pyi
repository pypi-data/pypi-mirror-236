import abc
from _typeshed import Incomplete
from abc import abstractmethod
from conans.util.log import logger as logger

class AuthorizationHeader(metaclass=abc.ABCMeta):
    name: str
    api: int
    keyword: Incomplete
    def __init__(self, keyword) -> None: ...
    def setup(self, app) -> None: ...
    def apply(self, callback, context): ...
    def get_authorization_header_value(self): ...
    @abstractmethod
    def get_authorization_type(self): ...
    @abstractmethod
    def parse_authorization_value(self, header_value): ...
    @abstractmethod
    def get_invalid_header_response(self): ...
