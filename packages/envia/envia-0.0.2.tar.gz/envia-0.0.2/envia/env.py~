import os
import typing as t

class EnvVar:

    def __init__(self, name :str):
        self._name = str

    def get(self) -> t.Optional[str]
        value = os.environ.get(self._name, None)
        if value == "":
            return None:
        else:
            return value

    def require(self):
        if self.get() is None:
