{
	"version": "0.1",
	"targethardware": [
    {
      "name": "raspberry_pi",
      "tty": "/dev/ttyAMA0",
      "apt_source": "deb http://archive.amescon.com/ rpi_raspbian main",
      "kernelversionre": "Linux (?P<name>.*?) (?P<version>\\d+.\\d+.\\d+-.*?) #(?P<buildnum>\\d+).*?(?P<arch>\\S+) GNU/Linux",
      "fullname": "Raspberry Pi",
      "defaultuser": "pi",
      "defaultpassword": "raspberry",
      "reset_pin": {
        "number": 17
      },
      "boot_pin": {
        "number": 4     
      },
      "tdi_pin": {
        "number": 22
      },
      "tms_pin": {
        "number": 23
      },
      "tdo_pin": {
        "number": 24
      },
      "tck_pin": {
        "number": 25
      },
      "devicetree_overlay": "pilot_pi.dtbo",
      "hardware": {
        "runcheck": "cat /sys/firmware/devicetree/base/model",
        "checkre": "Raspberry Pi.*"
      }
    },
    {
      "name": "coral",
      "tty": "/dev/ttymxc0",
      "apt_source": "deb http://archive.amescon.com/ coral_mendel main",
      "kernelversionre": "Linux (?P<name>.*?) (?P<version>\\d+.\\d+.\\d+-.*?) .*?(?P<arch>\\S+) GNU/Linux",
      "fullname": "Google Coral",
      "defaultuser": "mendel",
      "defaultpassword": "mendel",
      "reset_pin": {
        "number": 154
      },
      "boot_pin": {
        "number": 155
      },
      "tdi_pin": {
        "number": 14
      },
      "tms_pin": {
        "number": 73
      },
      "tdo_pin": {
        "number": 138
      },
      "tck_pin": {
        "number": 140
      },
      "devicetree_overlay": "pilot_coral.dtbo",
      "hardware": {
        "runcheck": "cat /etc/os-release | grep ID=mendel",
        "checkre": "ID=mendel"
      }
    }
  ]
}
