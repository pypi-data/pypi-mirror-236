rule infer
  command = /usr/bin/python3 -m pytype.single --imports_info $imports --module-name $module --platform linux -V 3.10 -o $out --no-report-errors --nofail --quick $in
  description = infer $module
rule check
  command = /usr/bin/python3 -m pytype.single --imports_info $imports --module-name $module --platform linux -V 3.10 -o $out --analyze-annotated --nofail --quick $in
  description = check $module
